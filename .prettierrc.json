{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "printWidth": 80,
  "arrowParens": "always",
  "endOfLine": "lf",
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react.*$",
    "^[^.]",
    "pages",
    "components",
    "hooks",
    "services",
    "utils",
    "types$",
    "constants",
    ".*.(png|svg|jpe?g|gif)$",
    "style$",
    "^./.*"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
